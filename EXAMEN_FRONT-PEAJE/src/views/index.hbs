<body class="g-sidenav-show   bg-gray-100">

  <div class="min-height-300 bg-primary position-absolute w-100"></div>
  <main class="main-content position-relative border-radius-lg ">


    <div class="container-fluid py-4">
      <div class="row">
        <div class="col-12">
          <div class="card">

            <div class="card-header pb-0">
              <div class="d-lg-flex">
                <div>
                  <h5 class="mb-0">Todas las exportaciones</h5>
                </div>
                <div class="ms-auto my-auto mt-lg-0 mt-4">
                  <div class="ms-auto my-auto">
                    <button type="button" class="btn bg-gradient-primary btn-sm mb-0" data-bs-toggle="modal"
                      data-bs-target="#exportacion">
                      nueva exportación
                    </button>
                  </div>
                </div>
              </div>
            </div>
            <div class="card-body px-0 pb-0">
              <div class="table-responsive">
                <table class="table table-flush" id="products-list">
                  <thead class="thead-light">
                    <tr>
                      <th>ID</th>
                      <th>PLACA</th>
                      <th>PEAJE</th>
                      <th>DESCRIPCIÓN</th>
                      <th>CATEGORIA</th>
                      <th>F.REGISTRO</th>
                      <th>PRECIO</th>
                      <th>CONTROLES</th>
                    </tr>
                  </thead>
                  <tbody>
                    {{#if datos.length}}
                    {{#each datos}}
                    <tr>
                      <td class="text-sm" id="">{{idPeaje}}</td>
                      <td class="text-sm" id="placa">{{placa}}</td>
                      <td class="text-sm" id="peaje">{{peaje}}</td>
                      <td class="text-sm" id=" ">{{descripcion}}</td>
                      <td class="text-sm" id=" ">{{idCategoria}}</td>
                      <td>
                        <span class="badge badge-danger ">{{fechaRegistro}}</span>
                      </td>
                      <td class="text-sm">{{precio}}</td>
                      <td class="text-sm">


                          <button class="btn btn-outline-primary btn-sm export mb-0 mt-sm-0 mt-1" data-bs-toggle="modal" id="editar"  data-bs-target="#exportacionEdit"  onclick="Update({{idPeaje}})" >Editar</button>

                        <button onclick="Eliminar({{idPeaje}})" class="btn bg-gradient-primary btn-sm mb-0"
                          type="submit">Eliminar</button>

                      </td>

                    </tr>
                    {{/each}}
                    {{else}}
                    {{/if}}
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </main>


