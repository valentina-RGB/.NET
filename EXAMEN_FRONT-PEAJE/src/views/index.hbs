  <!-- Modal -->
  <div class="modal fade" id="exportacion" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <h1 class="modal-title fs-5" id="exampleModalLabel">Crear exportación</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
        <div class="modal-body">

          <body>
            <div class="container mt-5">
              <form id="myForm" novalidate  method="post">
                <div class="row">
                  <div class="col-6">
                    <label for="precio" class="form-label">Placa</label>
                    <input class="form-control" id="placa" name="placa"  placeholder="EEA450" pattern="[A-Za-z]{3}[0-9]{3}" required/>
                    {{!-- <div class="invalid-feedback" id="precio-feedback">No se encontró un precio para la selección hecha.
                    </div> --}}
                  </div>
                  <div class="col-6">
                    <label for="peaje" class="form-label">Nombre del Peaje</label>
                    <input class="form-control" list="peajeList" id="peajes" name="peajes"
                      placeholder="Seleccione un peaje" required />
                    <datalist id="peajeList">
                      {{#each datospeaje}}
                      <option value="{{this.peaje}}">{{this.peaje}}</option>
                      {{/each}}
                    </datalist>
                  </div>
                  <div class="col-6">
                    <label for="categoria" class="form-label">Categoria</label>
                    <input class="form-control" list="categoriaList" id="categorias" name="categorias"
                      placeholder="Seleccione una categoría" required />
                    <datalist id="categoriaList">
                      {{#each datospeaje}}
                      <option value="{{this.idcategoriatarifa}}">{{this.idcategoriatarifa}}</option>
                      {{/each}}
                    </datalist>
                  </div>
                  <div class="col-6">
                    <label for="precio" class="form-label">Precio</label>
                    <input class="form-control" id="precio" name="precio" placeholder="1000" required readonly />
                    <div class="invalid-feedback" id="precio-feedback">No se encontró un precio para la selección hecha.
                    </div>
                  </div>
                    <div class="col-12">
                    <label for="precio" class="form-label">Descripcion</label>
                    <input class="form-control" id="descripcion" name="descripcion" placeholder="EE400" required  />
                    {{!-- <div class="invalid-feedback" id="precio-feedback">No se encontró un precio para la selección hecha.
                    </div> --}}
                  </div>
                </div>
                <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
                  <button type="submit" class="btn btn-primary" id="submitBtn" disabled>Guardar</button>
                </div>
              </form>

            </div>
        </div>
      </div>
      </div>
      </div>

